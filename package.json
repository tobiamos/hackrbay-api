{
  "name": "hackerbay-api",
  "version": "1.0.0",
  "description": "Api for hackerbay test",
  "main": "./src/app.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "test": "./node_modules/.bin/cross-env NODE_ENV=test ./node_modules/.bin/mocha \"./src/test/**/*.spec.js\" --timeout 15000 --exit",
    "coverage": "./node_modules/.bin/cross-env NODE_ENV=test nyc ./node_modules/.bin/mocha \"./src/test/**/*.spec.js\" --timeout 15000 --exit",
    "lint": "eslint \"./src/**/*.js\"",
    "lint:fix": "eslint \"./src/**/*.js\" --fix",
    "start": "node ./src/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tobiamos/hackrbay-api.git"
  },
  "keywords": [
    "Node",
    "Express",
    "JWT",
    "Json-Patch"
  ],
  "author": "Amos Tobi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tobiamos/hackrbay-api/issues"
  },
  "homepage": "https://github.com/tobiamos/hackrbay-api#readme",
  "dependencies": {
    "chalk": "^2.4.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-joi-validator": "^2.0.0",
    "image-downloader": "^3.4.1",
    "image-type": "^3.0.0",
    "joi": "^13.4.0",
    "joi-nochange": "^3.0.0",
    "jsonpatch": "^3.0.1",
    "jsonwebtoken": "^8.3.0",
    "mime-validator": "^1.0.0",
    "mongodb": "^3.1.1",
    "mongoose": "^5.2.5",
    "morgan": "^1.9.0",
    "pmx": "^1.6.7",
    "rimraf": "^2.6.2",
    "sharp": "^0.20.5",
    "swagger-ui-express": "^3.0.10",
    "valid-url": "^1.0.9",
    "winston": "2.4.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.2.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "nyc": "^12.0.2",
    "eslint-plugin-import": "^2.12.0"
  }
}
